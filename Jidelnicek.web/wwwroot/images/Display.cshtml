@{
}

@model IEnumerable<string>

<h2>Image Gallery</h2>

@foreach (var fileName in Model)
{
    <div>
        <img src="@Url.Action("DisplayImage", "Images", new { fileName = fileName })" alt="Image" />
    </div>
}

